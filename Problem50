public class a 
{
public static void main(String args[])
	{
		boolean prime = true;
		int z = 0;
		for (int x = 2; x <= 200; x ++) 
		{
			for (int y = 2; y <= 200; y++)
			{ 
					if (x % y == 0)
					{
						prime = false;
					}
					if (prime == true)
					{
						z = x % y;	
					}
					
			}

			System.out.println(z);
		}	
	}
}
